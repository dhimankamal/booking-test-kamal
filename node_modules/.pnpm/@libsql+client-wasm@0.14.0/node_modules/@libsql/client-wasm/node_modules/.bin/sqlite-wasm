#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/kamal/Documents/Projects/2 repos/booking-test-kamal/node_modules/.pnpm/@libsql+client-wasm@0.14.0/node_modules/@libsql/client-wasm/node_modules/@libsql/libsql-wasm-experimental/bin/node_modules:/Users/kamal/Documents/Projects/2 repos/booking-test-kamal/node_modules/.pnpm/@libsql+client-wasm@0.14.0/node_modules/@libsql/client-wasm/node_modules/@libsql/libsql-wasm-experimental/node_modules:/Users/kamal/Documents/Projects/2 repos/booking-test-kamal/node_modules/.pnpm/@libsql+client-wasm@0.14.0/node_modules/@libsql/client-wasm/node_modules/@libsql/node_modules:/Users/kamal/Documents/Projects/2 repos/booking-test-kamal/node_modules/.pnpm/@libsql+client-wasm@0.14.0/node_modules/@libsql/client-wasm/node_modules:/Users/kamal/Documents/Projects/2 repos/booking-test-kamal/node_modules/.pnpm/@libsql+client-wasm@0.14.0/node_modules/@libsql/node_modules:/Users/kamal/Documents/Projects/2 repos/booking-test-kamal/node_modules/.pnpm/@libsql+client-wasm@0.14.0/node_modules:/Users/kamal/Documents/Projects/2 repos/booking-test-kamal/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/kamal/Documents/Projects/2 repos/booking-test-kamal/node_modules/.pnpm/@libsql+client-wasm@0.14.0/node_modules/@libsql/client-wasm/node_modules/@libsql/libsql-wasm-experimental/bin/node_modules:/Users/kamal/Documents/Projects/2 repos/booking-test-kamal/node_modules/.pnpm/@libsql+client-wasm@0.14.0/node_modules/@libsql/client-wasm/node_modules/@libsql/libsql-wasm-experimental/node_modules:/Users/kamal/Documents/Projects/2 repos/booking-test-kamal/node_modules/.pnpm/@libsql+client-wasm@0.14.0/node_modules/@libsql/client-wasm/node_modules/@libsql/node_modules:/Users/kamal/Documents/Projects/2 repos/booking-test-kamal/node_modules/.pnpm/@libsql+client-wasm@0.14.0/node_modules/@libsql/client-wasm/node_modules:/Users/kamal/Documents/Projects/2 repos/booking-test-kamal/node_modules/.pnpm/@libsql+client-wasm@0.14.0/node_modules/@libsql/node_modules:/Users/kamal/Documents/Projects/2 repos/booking-test-kamal/node_modules/.pnpm/@libsql+client-wasm@0.14.0/node_modules:/Users/kamal/Documents/Projects/2 repos/booking-test-kamal/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@libsql/libsql-wasm-experimental/bin/index.js" "$@"
else
  exec node  "$basedir/../@libsql/libsql-wasm-experimental/bin/index.js" "$@"
fi
